{
    "manifest_version": 3,
    "name": "원신 쿠키 리더",
    "description": "미호요 API 사용에 필요한 쿠키를 자동으로 읽어줍니다",
    "version": "0.1",
    "icons": {
        "16": "/images/16-1.png",
        "48": "/images/48-1.png",
        "128": "/images/128-1.png"
    },
    "host_permissions": [
        "https://genshin.hoyoverse.com/"
      ],
    "permissions": [
        "cookies"
    ],
    
    "action": {
        "default_popup": "popup.html"
    }
}